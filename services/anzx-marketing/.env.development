# Development Environment Configuration
NODE_ENV=development

# Core API Configuration
CORE_API_URL=http://localhost:8000
NEXT_PUBLIC_CORE_API_URL=http://localhost:8000

# Chat Widget
CHAT_WIDGET_URL=http://localhost:3001
NEXT_PUBLIC_CHAT_WIDGET_URL=http://localhost:3001

# Firebase Configuration (Development)
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=anzx-ai-platform-dev
NEXT_PUBLIC_GOOGLE_CLOUD_PROJECT=anzx-ai-platform-dev
AGENT_SPACE_URL=https://agentspace.googleapis.com
NEXT_PUBLIC_AGENT_SPACE_URL=https://agentspace.googleapis.com

# Analytics (Development - use test IDs)
NEXT_PUBLIC_GA_MEASUREMENT_ID=
NEXT_PUBLIC_CLARITY_PROJECT_ID=

# Sentry (Development)
SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_ENVIRONMENT=development

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_CHAT_WIDGET=true
NEXT_PUBLIC_ENABLE_AGENT_DEMO=true

# API Rate Limiting
API_RATE_LIMIT=100
API_RATE_WINDOW=60000

# Debug
NEXT_PUBLIC_DEBUG_MODE=true
